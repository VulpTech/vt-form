{
    "name": "vt-form",
    "version": "0.1.0",
    "type": "module",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/VulpTech/vt-form.git"
    },
    "keywords": [
        "vue3",
        "vue",
        "vuejs"
    ],
    "files": [
        "dist"
    ],
    "main": "./dist/vt-form.umd.cjs",
    "module": "./dist/vt-form.js",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/vt-form.js",
            "require": "./dist/vt-form.umd.cjs"
        },
        "./vt-form.css": {
            "import": "./dist/vt-form.css",
            "require": "./dist/vt-form.css"
        }
    },
    "types": "./dist/index.d.ts",
    "scripts": {
        "dev": "vite",
        "build": "vue-tsc && vite build",
        "preview": "vite preview",
        "test:unit": "vitest",
        "build-only": "vite build",
        "type-check": "vue-tsc --build",
        "preinstall": "npx only-allow pnpm"
    },
    "peerDependencies": {
        "vue": "^3.0.0",
        "zod": "^3.24.1"
    },
    "devDependencies": {
        "@types/jsdom": "^21.1.7",
        "@types/node": "^22.10.2",
        "@vitejs/plugin-vue": "^5.2.1",
        "@vue/test-utils": "^2.4.6",
        "autoprefixer": "^10.4.20",
        "jsdom": "^25.0.1",
        "tailwindcss": "^3.4.17",
        "typescript": "~5.6.3",
        "vite": "^6.0.3",
        "vite-plugin-dts": "^4.3.0",
        "vite-plugin-vue-devtools": "^7.6.8",
        "vitest": "^2.1.5",
        "vue-tsc": "^2.1.10"
    },
    "dependencies": {
        "@internationalized/date": "^3.6.0",
        "@vueuse/core": "^12.0.0",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "lucide-vue-next": "^0.468.0",
        "radix-vue": "^1.9.11",
        "tailwind-merge": "^2.5.5",
        "tailwindcss-animate": "^1.0.7"
    }
}